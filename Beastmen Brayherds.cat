    <profile name="Giant´s club" hidden="false" id="742-e3dd-26f9-778f" typeId="cc88-6a7d-41c9-d63e" typeName="Weapon">
      <characteristics>
        <characteristic name="Free-Text Description" typeId="47f2-ecee-cae0-9ef9"/>
