            <characteristic name="Number" typeId="8a2b-690b-e552-6aa0"/>
            <characteristic name="Type" typeId="c2ca-5fd1-5e9d-bc90">Hex</characteristic>
            <characteristic name="Casting Value" typeId="d84d-3b8b-654a-9e1a">9+</characteristic>
            <characteristic name="Range" typeId="1043-a0ad-2909-dd28">Self</characteristic>
            <characteristic name="Effect" typeId="64ba-31-acf0-5a">Remains in Play. Whilst this spell is in play, enemy Wizards that are within 24&quot; of the caster&apos;s model when attempting to cast a spell must increase that spell&apos;s casting value by 2.</characteristic>
          </characteristics>
        </profile>
        <profile name="Fury of Khaine" hidden="false" id="cc2b-15f2-6542-8208" typeId="8232-ae14-b1f6-b4df" typeName="Spell">
