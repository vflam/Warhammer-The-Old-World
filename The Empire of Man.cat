    <profile name="Wizard Lord" hidden="false" id="d79e-c07c-4842-1d86" typeId="b070-143a-73f-2772" typeName="Model">
      <characteristics>
        <characteristic name="M" typeId="cd3b-a5a4-e185-5a9d">4</characteristic>
